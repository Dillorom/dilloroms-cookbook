
<h1>All Recipes:</h1>
<% @recipes.each do |recipe| %>
<ul><a href="/recipes/<%= recipe.slug %>"><%= recipe.name %></a> by recipe.user.username </ul>
<% end %>

<br></br>
<p><a href="/recipes/new">Add Recipe</a></p>
<p><a href="/">Home</a></p>
<% if !logged_in? %>
  <p><a href="/login">Log In</a></p>
<% else %>
  <p><a href="/logout">Log Out</a></p>
  <% end %>
